<script type="text/javascript">

  function makeid(length) {
      var result           = '';
      var characters       = 'ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789';
      var charactersLength = characters.length;
      for ( var i = 0; i < length; i++ ) {
          result += characters.charAt(Math.floor(Math.random()  *
              charactersLength));
      }
      return result;
  }

  var files;



  $(document).ready(function(){
    $('#access_token').val(makeid(15));
      $("#file-input").on("change", function(e){
          let fdata = new FormData();

          files = this.files;
          console.log(files);
          $.each(files, function(i, file) {
              fdata.append("file[" + i + "]", file);
          });
          fdata.append("domain", "momentum-15.myshopify.com");
          fdata.append("access_token", $('#access_token').val());
          console.log(fdata);

          $.ajax({
              url: "https://momentum-dubai.com/apps/unified-momentum/lead-images",
              // url: route('https://566e-182-176-83-28.ap.ngrok.io/lead-images'), for mydevice
              type: "post",
              data: fdata, //add the FormData object to the data parameter
              processData: false, //tell jquery not to process data
              contentType: false, //tell jquery not to set content-type
              success: function (data, status) {
                if (data.status == '1') {
                      $('.info').hide();
                  } else {
                      let errors = '<ul>';
                      $.each(data.error, function (key, value) {
                          errors += '<li>' + value + '</li>';
                      });
                      errors += '</ul>';
                      $('.info-message').html(errors);
                      $('.info').show();
                  }
              },
              error: function (jqxhr, status, errorMessage) {
                  let errors = '<ul><li>Something went wrong, please try again later.</li></ul>';
                  $('.info-message').html(errors);
                  $('.info').show();
                  $('#contact_form_save').show();
              }
          });
      });
  });
</script>

